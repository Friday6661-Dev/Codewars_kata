      "source": [
        "# Multiple of 3 or 5"
      ]
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "wxE1kV8XDLf7"
      },
      "source": [
        "def solution(number):\n",
        "    result = []\n",
        "    for i in range(3, number):\n",
        "        if i % 3 == 0:\n",
        "            result.append(i)\n",
        "        elif i % 5 == 0:\n",
        "            result.append(i)\n",
        "        else:\n",
        "            continue\n",
        "    return sum(result)"
      ],
      "execution_count": 9,
      "outputs": []
    },
